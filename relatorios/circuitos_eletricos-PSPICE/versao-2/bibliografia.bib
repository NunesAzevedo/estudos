% exemplos de todos os tipos de bibliografia citaveis https://www.bibtex.com/e/entry-types/
@book{exemploLivro,
    author = {autor1 and autor2},
    title = {titulo do livro},
    publisher = {editora},
    year = {ano de publicacao},
    edition = {10},
    address = {local de publicacao}
}
@article{exemploArtigo,
    author = {autor},
    title = {titulo},
    journal = {revista},
    year = {ano},
    volume  = {volume},
    number  = {numero},
    pages   = {x-y}
}
@misc{exemploWeb,
    author = {autor com acentua{\c{c}}{\~a}o},
    title = {titulo},   
    year = {ano},
    url = {site},
    note = {Acesso em: dd mmm. aaaa}
}